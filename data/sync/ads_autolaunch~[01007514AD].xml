<?xml version='1.0' encoding='UTF-8' ?>
<barista_record_table>ADS_AUTOLAUNCH</barista_record_table>
<barista_admin_records>
	<barista_record>
		<PROCESS_ID>BUILDSPROC</PROCESS_ID>
		<DESCRIPTION>Init for building SPROC defs</DESCRIPTION>
		<PROGRAM_PATH>[+DIR_PGM]adx_build_sproc.aon</PROGRAM_PATH>
		<PROGRAM_ARGS>-u[+USER_ID]</PROGRAM_ARGS>
		<PROCESS_TYPE>O</PROCESS_TYPE>
		<PROCESS_STATUS>N</PROCESS_STATUS>
		<ASC_COMP_ID>01007514</ASC_COMP_ID>
		<ASC_PROD_ID>AD </ASC_PROD_ID>
		<COMMENTS>This program adds SPROC definitions to the BASIS DD (procedures.dd and proc_params.dd), based on a hard-coded list of SPROCs used in Addon.  Eventually, list of SPROCs will be table-based.[$0a$]                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   </COMMENTS>
	</barista_record>
	<barista_record>
		<PROCESS_ID>SETPRINTER</PROCESS_ID>
		<DESCRIPTION>Set Printer Default by User</DESCRIPTION>
		<PROGRAM_PATH>[+DIR_PGM]adx_set_printer.aon</PROGRAM_PATH>
		<PROGRAM_ARGS>-u[+USER_ID]</PROGRAM_ARGS>
		<PROCESS_TYPE>O</PROCESS_TYPE>
		<PROCESS_STATUS>N</PROCESS_STATUS>
		<ASC_COMP_ID>01007514</ASC_COMP_ID>
		<ASC_PROD_ID>AD </ASC_PROD_ID>
		<COMMENTS>Sets the default printer for this user and firm to the Barista default printer.[$0a$]                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                </COMMENTS>
	</barista_record>
</barista_admin_records>